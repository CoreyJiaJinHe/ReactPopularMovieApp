const mongoose = require ('mongoose');

const searchTerm = mongoose.Schema(
    {
        text:{
            type: String,
            required: [true],
        },
        count:{
            type: int,
        },
        poster_url:{
            type: url,
        },
        id:{
            type: int,
            required: [true],
        },
    },
);

module.exports=mongoose.model('searchTerm',searchTerm);